{
  "name": "carbotracker",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "carbotracker",
  "sourceRoot": "apps/carbotracker/src",
  "tags": [],
  "targets": {
    "deploy": {
      "dependsOn": [
        {
          "target": "build"
        }
      ],
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/carbotracker",
        "commands": [
          {
            "command": "npx firebase deploy"
          }
        ]
      }
    },
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "apps/carbotracker/.dist",
        "index": "apps/carbotracker/src/index.html",
        "main": "apps/carbotracker/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/carbotracker/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/carbotracker/public"
          }
        ],
        "styles": ["apps/carbotracker/src/styles.scss"],
        "scripts": [],
        "serviceWorker": true,
        "ngswConfigPath": "apps/carbotracker/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/carbotracker/src/environments/environment.ts",
              "with": "apps/carbotracker/src/environments/environment.development.ts"
            }
          ]
        },
        "pr-preview": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/carbotracker/src/environments/environment.ts",
              "with": "apps/carbotracker/src/environments/environment.pr-preview.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "carbotracker:build:production"
        },
        "development": {
          "buildTarget": "carbotracker:build:development"
        }
      },
      "defaultConfiguration": "development",
      "continuous": true
    },
    "serve:firebase-emulated": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx firebase emulators:exec 'npx nx serve carbotracker' --ui --import=firebase-data/development -c ./firebase.json",
        "cwd": "apps/carbotracker"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "carbotracker:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/carbotracker/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "carbotracker:build"
      }
    }
  }
}
